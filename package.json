    "@types/react-dom": "^16.9.8",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-types": "^6.26.0",
    "dtslint": "^3.6.0",
    "enzyme": "^3.10.0",
